  margin: 0 auto;
}
/*模块-公共样式*/
.biaoti {
  width: 100%;
  height: 70px;
  margin-bottom: 120px;
  text-align: center;
}
.biaoti_line {
  width: 200px;
  height: 1px;
  background-color: RGB(220, 220, 220);
