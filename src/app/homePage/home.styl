/*分享*/
#fenxiang {
  height: 670px;
  box-sizing: border-box;
  padding-top: 94px;
  padding-bottom: 90px;
  background: #0F1011 url("../../images/newImg/share_bg.jpg") no-repeat center center / auto 100%;
}
#fenxiang .biaoti_name,#fenxiang .biaoti_name36{
  color: #fff;
}
#fenxiangcontent {
  text-align: center;
}
div[id^="fenxiang_"] {
  width: 100%;
  max-width: 204px;
  height: 250px;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  margin: 4px 1% 4px 1%;
  text-align: left;
  vertical-align: top;
}
@media screen and (max-width:700px) {
  div[id^="fenxiang_"] {
    width: 22%;
    height: 250px;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    margin: 4px 1% 4px 1%;
    text-align: left;
    vertical-align: top;
}
}
#fenxiang_4{
  margin-right: 4px;
}
div[id^="fx_"] {
}
div[id^="fx_"] img {
  width: 100%;
}
div[id^="fx_"]:hover {
  opacity: .8;
  //filter:alpha(opacity=80);
}
#shareMore {
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
}
#shareMore a {
  color: #fff;
  font-size: 20px;
  opacity: 0.5 !important;
  //filter: alpha(opacity=50); /* IE 8 */
  transition: all 1s ease 0s, opacity 1.5s ease 0s;
}
#shareMore a:hover{
  opacity: 1 !important;
  //filter: alpha(opacity=100); /* IE 8 */
  transition: all 1s ease 0s, opacity 1.5s ease 0s;
}
.fx_class {
  width: 50%;
  height: 28px;
  background-color: white;
  opacity: 0.5;
  //filter: alpha(opacity=50); /* IE 8 */
  //filter: alpha(opacity=50); /* IE 4-7 */
  border-bottom-right-radius: 14px;
  -moz-border-radius-bottomright: 14px;
  -webkit-border-bottom-right-radius: 14px;
  border-top-right-radius: 14px;
  -moz-border-radius-topright: 14px;
  -webkit-border-top-right-radius: 14px;
  font-size: 18px;
  color: #16A085;
  padding-left: 8px;
  line-height: 29px;
}
.fx_biaoti {
  color: white;
  font-size: 26px;
}
.fx_summary {
  color: #7e7c7c;
  text-overflow: ellipsis;
  overflow: hidden;
  line-height: 22px;
  height: 120px;
  margin-top: 10px;
  padding: 0 10px;
}
.fx_summary .title{
  height:43px;
  overflow:hidden;
  text-overflow:ellipsis;
  display block;
  font-size: 16px;
  color: #fff;
}
