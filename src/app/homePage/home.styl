.membertxt {
  width: 200px;
  height: 100px;
  margin-top: 10px;
  text-align: center;
  line-height: 20px;
}
.membername {
  font-size: 16px;
  line-height: 40px;
}
.memberlingyu {
  color: #a09e9e;
}
.tuanduiline {
  width: 20px;
}
.membertel {
  top: 65px;
  position: relative;
  font-size: 16px;
  color: white;
}
.memberemail {
  width: 90%;
  display: block;
  top: 70px;
  left: 5%;
  position: relative;
  color: white;
}
.membertelmail {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  opacity: 0;
  background: #1e7fb7;
  -webkit-transition: all 0.4s ease-in-out;
  -moz-transition: all 0.4s ease-in-out;
  -o-transition: all 0.4s ease-in-out;
  -ms-transition: all 0.4s ease-in-out;
  transition: all 0.4s ease-in-out;

  -webkit-transform: rotate(180deg) scale(0);
  -moz-transform: rotate(180deg) scale(0);
  -o-transform: rotate(180deg) scale(0);
  -ms-transform: rotate(180deg) scale(0);
  transform: rotate(180deg) scale(0);

  -webkit-backface-visibility: hidden; /*for a smooth font */
}
