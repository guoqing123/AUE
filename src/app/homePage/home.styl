/*规范*/
#guifan {
  /*background-color: red;*/
  padding-top: 82px;
  padding-bottom: 55px;
}
#guifan_content {
  text-align: center;
}
.guifan_txt_class {
  vertical-align: bottom;
  position: relative;
  bottom: -24px;
  font-size: 16px;
}
#swing, #web, #phone, #pad {
  width: 142px;
  height: 240px;
  margin: auto 3%;
  vertical-align: middle;
  text-align: center;
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
#swing> .imgBox, #web> .imgBox, #phone> .imgBox, #pad> .imgBox {
  width:142px;
  height: 142px;
  line-height: 142px;
  text-align: center;
  border-radius: 142px;
  border: 1px solid #efefef;
  position: relative;
}
.imgBox:hover{
  box-shadow: 0 0 14px #efeff2;
}
.imgBox img{
  vertical-align: middle;
  max-width: 100%;
}
#phone> .imgBox img{
  width: 66%;
}
#web> .imgBox img{
  width: 40%;
}
#swing> .imgBox img,#pad> .imgBox img{
  width: 80%;
}
#swing {
  -moz-transition: all 0.3s ease 0;
  -webkit-transition: all 0.3s ease 0;
  -o-transition: all 0.3s ease 0;
  transition: all 0.3s ease 0;
  transition: all 0.3s linear 0s;
}
#web {
  -moz-transition: all 0.3s ease 0;
  -webkit-transition: all 0.3s ease 0;
  -o-transition: all 0.3s ease 0;
  transition: all 0.3s ease 0;
  transition: all 0.3s linear 0s;
}
#webhou {
  -webkit-transition: all 0.3s ease 0;
  -o-transition: all 0.3s ease 0;
  transition: all 0.3s ease 0;
  transition: all 0.3s linear 0s;
}
#phone {
  -webkit-transition: all 0.3s ease 0;
  -o-transition: all 0.3s ease 0;
  transition: all 0.3s ease 0;
  transition: all 0.3s linear 0s;
}
#pad {
  -moz-transition: all 0.7s ease 0;
  -webkit-transition: all 0.7s ease 0;
  -o-transition: all 0.7s ease 0;
  transition: all 0.7s ease 0;
  transition: all 0.6s linear 0s;
}
